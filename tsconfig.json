{
  "compilerOptions": {
    "moduleDetection": "force",
    "module": "Preserve",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "baseUrl": "./src",
    "paths": {
      "@controllers/*": ["controllers/*"],
      "@middleware/*": ["middleware/*"],
      "@utilities/*": ["utilities/*"],
      "@prisma/*": ["../prisma/*"],
      "@routes/*": ["routes/*"]
    }
  }
}
